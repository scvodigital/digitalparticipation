<div class="checkup-section b-padding-full detail" id="checkup-confirmation">
  <div class="checkup-section-header b-padding-full v-margin-full">
    <div class="container">
      <div class="container-content">
        {{> checkup_progress progress=99}}
        <h1>Please wait</h1>
        <p>
          Processing your answers...
          <span class="fas fa-fw fa-spinner fa-spin"></span>
        </p>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="container-content">
      {{#with @root.data.response ~}}
      {{!-- <iframe src="https://www.surveygizmo.eu/s3/90133620/SCVO-Digital-Checkup?__no_style=true&s1={{score_leadership_culture_and_skills}}&s2={{score_tools_and_equipment}}&s3={{score_content_marketing_and_data}}&s4={{score_cyber_resilience}}&overall={{score_total}}&income_band={{urlencode income_band}}&topic={{urlencode topic}}&name={{urlencode name}}&email={{urlencode email}}&web_organisation={{urlencode organisation}}&area={{urlencode area}}&postcode={{urlencode postcode}}&next_project={{urlencode next_project}}&barriers={{urlencode barriers}}&support={{#if support}}Yes+please{{else}}No+thanks{{/if}}&rid={{@root.data.rid}}{{#if @root.data.response.sid}}&sid={{@root.data.response.sid}}{{/if}}" frameborder="0" width="700" height="10" style="overflow:hidden;visibility:hidden;"></iframe> --}}
      <script type="text/javascript">
        goToResults();
      </script>
      {{/with ~}}
    </div>
  </div>
</div>
<script type="text/javascript">
  function goToResults() {
    window.location.href = '/skills/checkup/results?rid={{#if @root.data.response.sid}}{{@root.data.response.sid}}{{else}}{{@root.data.rid}}{{/if}}';
  }
</script>
