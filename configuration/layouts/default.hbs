  <link type="text/css" rel="stylesheet" href="{{~>asset_url url='main.css'}}">
</head>
<body class="{{#if request.params.path}}{{replace request.params.path '_' ' '}}{{else}}home{{/if}}">
  {{!-- <div class=" hide-sm hide-md">
    {{> components_scvo_bar}}
  </div> --}}
  <main>
    <header class="navbar header">
      <div class="container">
        <section class="navbar-section pull-left">
          <span class="navbar__title catalog-title">
            <a href="/" class="brand-logo">
              <span class="digital">Digital</span>
              <span class="participation">Participation</span>
              <span class="charter">Charter</span>
            </a>
          </span>
        </section>
        <section class="navbar-section hide-sm pull-right">
          <nav id="main-tab-bar" class="mdc-tab-bar t-margin-full t-padding-full" [menu]="'main'" [menulinkselector]="''" [menulabelselector]="'span.mdc-tab--label'" [menuactiveclass]="'active'">
            {{#each context.metaData.menus.main ~}}
            <div class="dropdown">
              <a class="mdc-tab{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-tab--active{{/if}}" href="{{path}}"><span class="mdc-tab--label">{{{label}}}</span></a>
            </div>
            {{/each ~}}
          </nav>
        </section>
        <section class="navbar-section pull-right">
          <div class="mobile-header-buttons show-lg" style="margin-top:1.75rem; margin-right:-0.4rem">
            <a href="#" class="btn btn-link no-margin" aria-label="Menu" data-component="TasksTrigger"
              data-TasksTrigger='{
                click: {
                  tasks: [
                    {
                      type: "elementManipulator",
                      config: {
                        ".mobile-menu": {
                          "removeClass": "mobile-menu_closed mobile-menu_closing",
                          "addClass": "mobile-menu_open"
                        }
                      }
                    }
                  ]
                }
              }'>
              <span class="far fa-bars fa-2x" aria-hidden="true"></span>
            </a>
          </div>
        </section>
      </div>
    </header>

    <div class="mobile-menu mobile-menu_closed">
      <aside class="mobile-contents sidebar bg-primary">
        <header class="navbar header">
          <section class="navbar-section">
            {{!-- <a href="/" class="navbar-brand" title="Scottish Council for Voluntary Organisations">
              <img src="{{~>asset_url url='images/logos/scvo-white.svg'~}}" alt="Scottish Council for Voluntary Organisations" />
            </a> --}}
          </section>
          <section class="navbar-section">
            <div class="mobile-header-buttons">
              <a href="#" class="btn btn-link" aria-label="Menu" data-component="TasksTrigger" data-TasksTrigger='{
                  click: {
                    tasks: [
                      {
                        type: "elementManipulator",
                        config: {
                          ".mobile-menu": {
                            removeClass: "mobile-menu_open",
                            addClass: "mobile-menu_closing"
                          }
                        }
                      },
                      {
                        type: "delay",
                        config: {
                          "200": [
                            {
                              type: "elementManipulator",
                              config: {
                                ".mobile-menu": {
                                  addClass: "mobile-menu_closed",
                                  removeClass: "mobile-menu_closing"
                                }
                              }
                            }
                          ]
                        }
                      }
                    ]
                  }
                }'>
                <span class="far fa-times fa-2x" aria-hidden="true"></span>
              </a>
            </div>
          </section>
        </header>
        <div class="b-margin-full v-padding-full">
          <ul class="menu b-padding-full">
            {{#each @root.context.metaData.menus.main ~}}
            <li class="menu-item">
              <a href="{{stripTrailingSlash path}}" class="{{#if (regexMatch @root.request.url.pathname route 'gi')}} active{{/if}}">
                {{#if icon ~}}
                <span class="far fa-fw fa-{{icon}}" aria-hidden="true"></span>
                {{/if ~}}
                {{{label}}}
              </a>
            </li>
            {{/each ~}}
          </ul>
          {{!-- <h5 class="x-padding">Our sites</h5>
          {{> components_scvo_bar}} --}}
        </div>
      </aside>
    </div>

    <div class="container">
      {{{layoutParts.body}}}
    </div>
  </main>

  <footer>
    <div class="footer-main">
      <div class="container">
        <div class="columns">
          <div class="column col-3 col-md-6 col-sm-12">
            <h3>Contact</h3>
            <ul class="fa-ul">
              <li>
                <span class="fa-li"><span class="fas fa-envelope" aria-hidden="true"></span></span>
                <a href="mailto:digital@scvo.org.uk">digital@scvo.org.uk</a>
              </li>
              <li>
                <span class="fa-li"><span class="fab fa-twitter" aria-hidden="true"></span></span>
                <a href="https://twitter.com/digiscot" rel="noopener">Twitter</a>
              </li>
            </ul>
          </div>
          <div class="column col-3 col-md-6 col-sm-12">
            <h3>Help</h3>
            <ul>
              <li>
                <a href="/accessibility">Accessibility</a>
              </li>
              <li>
                <a href="/privacy">Privacy</a>
              </li>
              <li>
                <a href="/cookies">Cookies</a>
              </li>
              <li>
                <a href="/terms">Terms &amp; conditions</a>
              </li>
            </ul>
          </div>
          <div class="column col-6 col-md-12">
            <h3>About</h3>
            <p>
              Scotland's Digital Participation Charter is run by the <a href="https://scvo.org">Scottish Council for Voluntary Organisations</a> with support from the <a href="https://www.gov.scot/policies/digital">Scottish Government</a>.
            </p>
          </div>
        </div>
      </div>
    </div>
    <div class="footer-copyright">
      <div class="container">
        <div class="columns">
          <div class="column col-8 col-md-12">
            <p>
              &copy; {{momentFormat (moment) "YYYY"}}. The Scottish Council for Voluntary Organisations (SCVO) is a Scottish Charitable Incorporated Organisation.<br />Charity registered in Scotland <a href="https://www.oscr.org.uk/about-charities/search-the-register/charity-details?number=SC003558">SC003558</a>. Registered office Mansfield Traquair Centre, 15 Mansfield Place, Edinburgh EH3&nbsp;6BB.
            </p>
          </div>
          <div class="column col-4 col-md-12">
            <a href="https://www.gov.scot/policies/digital" class="brand">
              <img src="{{~>asset_url url='images/logos/SG_Quaternary_govscot_RGB_reversed.png'~}}" alt="Scottish Government"  class="logo digital-scotland" />
            </a>
            <a href="https://scvo.org" title="Scottish Council for Voluntary Organisations" class="brand">
              <img src="{{~>asset_url url='images/logos/scvo-white.svg'~}}" alt="Scottish Council for Voluntary Organisations" class="logo scvo" />
            </a>
          </div>
        </div>
      </div>
    </div>
  </footer>

  {{#each @root.context.metaData.handlebars.frontend.configs ~}}
