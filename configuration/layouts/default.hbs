  <style type="text/css">
  @font-face {
  font-family: 'Ingra';
  src: url('{{~>asset_url url='fonts/IngraSCVO-Regular.eot'~}}');
  src: url('{{~>asset_url url='fonts/IngraSCVO-Regular.eot?#iefix'~}}') format('embedded-opentype'),
